{
    "name": "rndwiga/mpesa-sdk",
    "type": "library",
    "description": "An expressive mpesa implementation",
    "keywords": ["ai", "rndwiga","mpesa", "laravel", "php"],
    "authors": [
        {
            "name": "Raphael Ndwiga",
            "email": "raphael@raphaelndwiga.africa"
        }
    ],
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^3.9",
        "vlucas/phpdotenv": "^3.4",
        "phpseclib/phpseclib": "^2.0",
        "ramsey/uuid": "^3.8",
        "psr/log": "^3.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-zlib": "*"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "mockery/mockery": "2.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Rndwiga\\Mpesa\\": "src/"
        },
        "files": [
            "src/Utils/MpesaHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rndwiga\\Mpesa\\MpesaServiceProvider"
            ],
            "aliases": {
                "Mpesa": "Rndwiga\\Mpesa\\Facades\\Mpesa"
            }
        }
    }
}
