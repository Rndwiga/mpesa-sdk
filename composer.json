{
    "name": "rndwiga/project-mpesa",
    "type": "package",
    "description": "An expressive mpesa implementation",
    "keywords": ["ai", "rndwiga","mpesa"],
    "authors": [
        {
            "name": "Raphael Ndwiga",
            "email": "raphael@raphaelndwiga.africa"
        }
    ],
    "require": {
        "php": "^8.1",
        "rndwiga/mpesa": "@dev",
        "rndwiga/toolbox": "@dev",
        "monolog/monolog": "^1.24",
        "vlucas/phpdotenv": "^3.4",
        "phpseclib/phpseclib": "^2.0",
        "ramsey/uuid": "^3.8",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
      "ext-zlib": "*"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "mpesa",
            "options": {
                "symlink": true
            }
        },{
            "type": "path",
            "url": "Toolbox",
            "options": {
                "symlink": true
            }
        }
    ],
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "mockery/mockery": "2.0.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
