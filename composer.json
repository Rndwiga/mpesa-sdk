{
    "name": "rndwiga/project-mpesa",
    "type": "package",
    "description": "An expressive mpesa implementation",
    "keywords": ["ai", "rndwiga","mpesa"],
    "authors": [
        {
            "name": "Raphael Ndwiga",
            "email": "raphndwi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "rndwiga/mpesa": "@dev",
        "rndwiga/toolbox": "@dev",
        "monolog/monolog": "^1.24",
        "vlucas/phpdotenv": "^3.4",
        "phpseclib/phpseclib": "^2.0",
        "ramsey/uuid": "^3.8",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
